# ====================
# Flutter Aliases
# ====================
alias fpg='flutter pub get'
alias fpu='flutter pub upgrade'

alias fd='flutter doctor'
alias fc='flutter clean'

alias brun='dart run build_runner build --d'
alias wrun='dart run build_runner watch'
alias dcc='dart pub cache clean'

# ====================
# CocoaPods Aliases
# ====================
alias mpod='arch -x86_64 pod'
alias pinst='mpod install'
alias pru='mpod repo update'
